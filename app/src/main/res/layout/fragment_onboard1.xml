<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">


    <ImageView
        android:id="@+id/onboard1_dvd_background"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/onboard"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="285dp"/>

    <TextView
        android:id="@+id/onboard1_title_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="오늘의 음악 이야기"
        android:textSize="20sp"
        android:textColor="@color/black"
        android:textStyle="bold"
        android:layout_marginTop="149dp"
        android:layout_marginStart="30dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"/>

    <TextView
        android:id="@+id/onboard1_subtitle_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="친구들과 그날 그날의 노래를 공유해보세요!"
        android:textSize="13sp"
        android:textColor="#C4C4C4"
        android:layout_marginTop="5dp"
        app:layout_constraintTop_toBottomOf="@id/onboard1_title_tv"
        app:layout_constraintStart_toStartOf="@id/onboard1_title_tv"/>

    <ImageView
        android:id="@+id/onboard1_time_iv"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:src="@drawable/ob1_ellipse"
        android:layout_marginEnd="30dp"
        android:layout_marginBottom="6.57dp"
        app:layout_constraintBottom_toTopOf="@+id/onboard1_dvd_background"
        app:layout_constraintEnd_toEndOf="parent"/>

    <ImageView
        android:id="@+id/onboard1_time_union_iv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ob1_union"
        android:layout_marginEnd="29dp"
        android:layout_marginTop="13.5dp"
        app:layout_constraintTop_toTopOf="@id/onboard1_time_iv"
        app:layout_constraintEnd_toEndOf="parent"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="24h"
        android:textSize="20sp"
        android:textColor="@color/black"
        android:textStyle="bold"
        android:layout_marginTop="17.86dp"
        android:layout_marginEnd="44.28dp"
        app:layout_constraintTop_toTopOf="@id/onboard1_time_iv"
        app:layout_constraintEnd_toEndOf="parent"/>

    <ImageView
        android:layout_width="220dp"
        android:layout_height="175dp"
        android:src="@drawable/ob1_group"
        android:layout_marginTop="339dp"
        android:layout_marginStart="60dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="140dp"
        android:src="@drawable/ob1_union1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.505"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/onboard1_time_iv" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ob1_vector1"
        android:layout_marginStart="120dp"
        android:layout_marginTop="32dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/onboard1_time_iv"/>

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ob1_union2"
        android:layout_marginStart="75dp"
        android:layout_marginTop="170dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/onboard1_time_iv"/>

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ob1_subtract2"
        android:layout_marginTop="183dp"
        android:layout_marginStart="92dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/onboard1_time_iv"/>

    <ImageView
        android:id="@+id/onboard1_first_iv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/onboard_1"
        android:scaleType="fitXY"
        android:layout_marginBottom="80dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>



</androidx.constraintlayout.widget.ConstraintLayout>